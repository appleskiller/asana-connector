{
    "compilerOptions": {
        "target":"es5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "declaration": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "noLib": false,
        "rootDir": "src/",
        "outDir": "dist/"
    },
    "exclude": [
        "node_modules"
    ]
}